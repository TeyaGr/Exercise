package PrimitivesAndReferences;

public class Books {
static String title;
static String author;
public String getTitle() {
	return title;
}
public void setTitle(String title) {
	this.title = title;
}
public String getAuthor() {
	return author;
}
public void setAuthor(String author) {
	this.author = author;
}

Books(String title, String author){
	this.title = title;
	this.author = author;
}
public static void getDetails() {
	System.out.println("We have the book "+ " "+ title + "by the author"+ " "+ author);
}
}
