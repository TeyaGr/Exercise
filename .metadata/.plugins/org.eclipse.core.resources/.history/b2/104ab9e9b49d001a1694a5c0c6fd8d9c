package PrimitivesAndReferences;

public class Dog {

	public static void main(String[] args) {
		
		Dog[] pets;
		pets = new Dog[7];
		pets[0]= new Dog();
		pets[1]= new Dog();
		pets[2]= new Dog();
		pets[1].bark();
		pets[2].name("Luna");
	}
	public static void bark() {
		System.out.println("Woof Woof");
	

	}
	public static void name(String s) {
		System.out.println(s);
	}
	}

