
public class Proizvod {

public static int MultiplyNumber(int n) {
		if (n == 0) {
			return 0;
		} else {
			System.out.print(n%10  + " ");
			return n%10 * MultiplyNumber(n/10);
		}
}
public static void main(String[] args) {
	int i = 123;
	int first = i % 10;
	
	int second = i / 10;

	first = 12 % 10;
	second = 12 / 10;

	first = 1 % 10;
	second = 1 /10;

	System.out.println(MultiplyNumber(i));
}
}
