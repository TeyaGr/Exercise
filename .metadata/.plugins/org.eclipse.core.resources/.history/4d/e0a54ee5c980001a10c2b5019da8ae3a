
public class VariablesAndShit {

	public static void main(String[] args) {

		int a, b ;
		a = 5;
		b= 165;
		System.out.println( " a + b =" + (a+b));
		System.out.println("a-b = " + (a-b));
		System.out.println( "a * b =" + (a*b));
		System.out.println( "Ostatokot od b/a e" + " " + (b%a));
		System.out.println(a/b);
		
		
		
	}
}
