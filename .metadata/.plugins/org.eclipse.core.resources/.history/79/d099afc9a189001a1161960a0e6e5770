
public class MinFunc {

	
	public static int minFunction ( int a, int b) {
		int min;
		if (a>b) {
			min = b;
		}else {
			min = a;
		}

		return min;
	}
	public static void main(String[] args) {
		
		int a = 5;
		int b =10;
		int c = minFunction(a,b);
		System.out.println("The minimum value is " + c);
	}
}
